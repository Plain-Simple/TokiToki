#Future

tenseIs("Tense.FUTURE") AND personIs("Person.FIRST") AND numberIs("Number.SINGULAR"), replaceEnd("" "é")
tenseIs("Tense.FUTURE") AND personIs("Person.SECOND") AND numberIs("Number.SINGULAR"), replaceEnd("" "ás")
tenseIs("Tense.FUTURE") AND personIs("Person.THIRD") AND numberIs("Number.SINGULAR"), replaceEnd("" "á")
tenseIs("Tense.FUTURE") AND personIs("Person.FIRST") AND numberIs("Number.PLURAL"), replaceEnd("" "emos")
tenseIs("Tense.FUTURE") AND personIs("Person.SECOND") AND numberIs("Number.PLURAL"), replaceEnd("" "án")
tenseIs("Tense.FUTURE") AND personIs("Person.THIRD") AND numberIs("Number.PLURAL"), replaceEnd("" "án")

