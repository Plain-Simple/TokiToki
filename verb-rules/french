########IRREGULAR####################

#Imperfect

verbIs("ver") AND tenseIs("Tense.IMPERFECT"), conjugate("veer")
verbIs("ser") AND tenseIs("Tense.IMPERFECT") AND personIs("Person.FIRST") AND numberIs("Number.PLURAL"), replaceEnd("ser" "éramos")
verbIs("ser") AND tenseIs("Tense.IMPERFECT"), conjugate("erar")
verbIs("ir") AND tenseIs("Tense.IMPERFECT") AND personIs("Person.FIRST") AND numberIs("Number.PLURAL"), replaceEnd("ir" "íbamos")
verbIs("ir") AND tenseIs("Tense.IMPERFECT"), conjugate("ibar")

########REGULAR######################

#Present

endsIn("ar") AND tenseIs("Tense.PRESENT") AND personIs("Person.FIRST") AND numberIs("Number.SINGULAR"), replaceEnd("ar" "o")
endsIn("ar") AND tenseIs("Tense.PRESENT") AND personIs("Person.SECOND") AND numberIs("Number.SINGULAR"), replaceEnd("ar" "as")
endsIn("ar") AND tenseIs("Tense.PRESENT") AND personIs("Person.THIRD") AND numberIs("Number.SINGULAR"), replaceEnd("ar" "a")
endsIn("ar") AND tenseIs("Tense.PRESENT") AND personIs("Person.FIRST") AND numberIs("Number.PLURAL"), replaceEnd("ar" "amos")
endsIn("ar") AND tenseIs("Tense.PRESENT") AND personIs("Person.SECOND") AND numberIs("Number.PLURAL"), replaceEnd("ar" "an")
endsIn("ar") AND tenseIs("Tense.PRESENT") AND personIs("Person.THIRD") AND numberIs("Number.PLURAL"), replaceEnd("ar" "an")

endsIn("er") AND tenseIs("Tense.PRESENT") AND personIs("Person.FIRST") AND numberIs("Number.SINGULAR"), replaceEnd("er" "o")
endsIn("er") AND tenseIs("Tense.PRESENT") AND personIs("Person.SECOND") AND numberIs("Number.SINGULAR"), replaceEnd("er" "es")
endsIn("er") AND tenseIs("Tense.PRESENT") AND personIs("Person.THIRD") AND numberIs("Number.SINGULAR"), replaceEnd("er" "e")
endsIn("er") AND tenseIs("Tense.PRESENT") AND personIs("Person.FIRST") AND numberIs("Number.PLURAL"), replaceEnd("er" "emos")
endsIn("er") AND tenseIs("Tense.PRESENT") AND personIs("Person.SECOND") AND numberIs("Number.PLURAL"), replaceEnd("er" "en")
endsIn("er") AND tenseIs("Tense.PRESENT") AND personIs("Person.THIRD") AND numberIs("Number.PLURAL"), replaceEnd("er" "en")

endsIn("ir") AND tenseIs("Tense.PRESENT") AND personIs("Person.FIRST") AND numberIs("Number.SINGULAR"), replaceEnd("ir" "o")
endsIn("ir") AND tenseIs("Tense.PRESENT") AND personIs("Person.SECOND") AND numberIs("Number.SINGULAR"), replaceEnd("ir" "es")
endsIn("ir") AND tenseIs("Tense.PRESENT") AND personIs("Person.THIRD") AND numberIs("Number.SINGULAR"), replaceEnd("ir" "e")
endsIn("ir") AND tenseIs("Tense.PRESENT") AND personIs("Person.FIRST") AND numberIs("Number.PLURAL"), replaceEnd("ir" "imos")
endsIn("ir") AND tenseIs("Tense.PRESENT") AND personIs("Person.SECOND") AND numberIs("Number.PLURAL"), replaceEnd("ir" "en")
endsIn("ir") AND tenseIs("Tense.PRESENT") AND personIs("Person.THIRD") AND numberIs("Number.PLURAL"), replaceEnd("ir" "en")

